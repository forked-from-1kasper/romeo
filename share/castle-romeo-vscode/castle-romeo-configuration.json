{
    "brackets": [
        ["(", ")"],
        ["[", "]"]
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["[", "]"]
    ],
    "folding": {
        "offSide": true,
        "markers": {
            "start": "^[[:word:]']+",
            "end": "^[[:word:]']+"
        }
    },
    "wordPattern": "[^\\s\\(\\)\\:\\.\\,-][^\\s\\(\\)\\:\\.\\,]*"
}